<!DOCTYPE html>
<html lang="{{ Culture.Name }}">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    {% render_section "HeadMeta", required: false %}
    <title>{% page_title Site.SiteName %}</title>
    {% resources type: "Meta" %}

    <!-- Bootstrap core CSS -->
    <!-- {% style src:"https://maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" %} -->

    <link href="/TheSmallBusinessTheme/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet"/>

    <!-- Custom styles for this template -->
    <link href="/TheSmallBusinessTheme/css/modern-business.css" rel="stylesheet"/>
    <link href="/TheSmallBusinessTheme/css/Styles.css" rel="stylesheet" />
    <!--  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous"> -->

    {% resources type: "HeadScript" %}
    {% resources type: "HeadLink" %}
    {% resources type: "Stylesheet" %}

</head>

<body dir="{{ Culture.Dir }}">

    <!-- Navigation -->
    {% shape "menu", alias: "alias:main-menu", cache_id: "main-menu", cache_tag: "alias:main-menu" %}

    <!-- Header -->
    <header>
        {% render_section "Header", required: false %}
    </header>

    <!-- Main Body -->
    <div class="container">
        {% render_section "Content" %}
    </div>

    <!-- Footer -->
    <footer class="py-5 bg-dark">
        {% render_section "Footer", required: false %}
    </footer>

    <!-- Bootstrap core JavaScript -->


    <script src="/TheSmallBusinessTheme/vendor/jquery/jquery.min.js"></script>
    <script src="/TheSmallBusinessTheme/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!--  <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
     <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
     <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
        -->
    {% resources type: "FootScript" %}

</body>

</html>
